{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "sharepoint-template-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/sharepointTemplate/SharepointTemplateWebPart.js",
          "manifest": "./src/webparts/sharepointTemplate/SharepointTemplateWebPart.manifest.json"
        }
      ]
    },
    "custome-page-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/customePage/CustomePageWebPart.js",
          "manifest": "./src/webparts/customePage/CustomePageWebPart.manifest.json"
        }
      ]
    },
    "contact-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/contact/ContactWebPart.js",
          "manifest": "./src/webparts/contact/ContactWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "SharepointTemplateWebPartStrings": "lib/webparts/sharepointTemplate/loc/{locale}.js",
    "CustomePageWebPartStrings": "lib/webparts/customePage/loc/{locale}.js",
    "ContactWebPartStrings": "lib/webparts/contact/loc/{locale}.js"
  }
}
